{
  "name": "rolukja/vilt-crud-generator",
  "description": "A Composer package for automatically generating CRUD files for Laravel with Inertia.js and Vue.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Roman Lukjanow",
      "email": "romanlukjanow@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "laravel/framework": "^11.0",
    "inertiajs/inertia-laravel": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "ViltCrudGenerator\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ViltCrudGenerator\\Providers\\ViltCrudGeneratorServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
